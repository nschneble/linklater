{
  "name": "linklater",
  "private": true,
  "version": "1.0.0",
  "description": "A del.icio.us reinterpretation of Instapaper.",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace @linklater/api\" \"npm run dev --workspace @linklater/web\"",
    "lint": "npm run lint --workspaces",
    "test": "npm run test --workspaces"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
